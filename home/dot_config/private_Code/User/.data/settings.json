{
  "_editor.fontFamily": "'Noto Sans Mono', Consolas, 'Courier New', monospace",
  "breadcrumbs.enabled": false,
  "diffEditor.codeLens": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.renderSideBySide": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.fontSize": 12,
  "editor.formatOnPaste": true,
  "editor.guides.bracketPairs": "active",
  "editor.hover.above": false,
  "editor.inlineSuggest.enabled": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    80,
    88
  ],
  "editor.selectionClipboard": false,
  "editor.showFoldingControls": "mouseover",
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "bottom",
  "editor.stickyTabStops": true,
  "editor.suggest.preview": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.unicodeHighlight.allowedLocales": {
    "_os": true,
    "_vscode": true,
    "fi": true
  },
  "editor.unicodeHighlight.ambiguousCharacters": true,
  "editor.unicodeHighlight.invisibleCharacters": true,
  "editor.unicodeHighlight.nonBasicASCII": false,
  "explorer.autoReveal": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.incrementalNaming": "smart",
  "explorer.openEditors.sortOrder": "alphabetical",
  "files.associations": {
    "Vagrantfile": "ruby"
  },
  "files.enableTrash": false,
  "files.eol": "\n",
  "files.exclude": {
    "**/*.py[co]": true,
    "**/.vagrant": true,
    "**/__pycache__": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "git-graph.dialog.addTag.type": "Lightweight",
  "git-graph.dialog.merge.noCommit": true,
  "git-graph.maxDepthOfRepoSearch": 4,
  "git-graph.repository.commits.fetchAvatars": true,
  "git-graph.repository.commits.mute.commitsThatAreNotAncestorsOfHead": true,
  "git-graph.repository.fetchAndPrune": true,
  "git-graph.repository.fetchAndPruneTags": true,
  "git.countBadge": "tracked",
  "git.enableCommitSigning": true,
  "git.mergeEditor": true,
  "git.untrackedChanges": "separate",
  "github.copilot.enable": {
    "*": true,
    "markdown": true,
    "plaintext": false,
    "yaml": true
  },
  "gitlens.defaultDateFormat": "ddd, D MMM YYYY hh:mm",
  "gitlens.defaultDateShortFormat": "D MMM YYYY",
  "gitlens.defaultTimeFormat": "hh:mm",
  "go.useLanguageServer": true,
  "latex-workshop.view.pdf.viewer": "tab",
  "markdown-pdf.displayHeaderFooter": false,
  "markdown-pdf.highlightStyle": "github.css",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "powershell.integratedConsole.showOnStartup": false,
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.pycodestyleEnabled": false,
  "redhat.telemetry.enabled": false,
  "scm.alwaysShowRepositories": true,
  "scm.repositories.sortOrder": "name",
  "search.smartCase": true,
  "settingsSync.keybindingsPerPlatform": false,
  "telemetry.telemetryLevel": "off",
  "terraform.languageServer": {
    "args": [
      "serve"
    ],
    "external": true
  },
  "update.mode": "none",
  "window.titleBarStyle": "custom",
  "workbench.colorTheme": "GitHub Dark",
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.languageDetection": true,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.editor.untitled.labelFormat": "name",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.pdf": "latex-workshop-pdf-hook"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "newUntitledFile"
}
